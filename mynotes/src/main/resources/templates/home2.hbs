{{>header.hbs}}

<div class="home-view">

	<div class="container">

		<div class="row">

			<div class="col"></div>

		</div>


		<div class="row">

			<div class="col col-md-8"></div>

			<div class="col col-md-2">

				<ul class="nav justify-content-end">
					<li class="nav-item"><a class="nav-link active">Hi,
							{{username}}</a></li>

					<!-- <li class="nav-item"><img src="{{photoUrl}}"></li>	 -->

					<!-- <li class="nav-item"><a class="nav-link" href="/logout">Log
							out</a></li> -->
				</ul>
			</div>

			<div class="col col-md-2 dropdown">
				<img class="btn dropdown-toggle" src="{{photoUrl}}" 
					alt="dropdown image" data-toggle="dropdown" class="img-responsive">
				<ul class="dropdown-menu">
					<li class="nav-item"><a class="nav-link" href="#">Settings</a></li>
					<li class="nav-item"><a class="nav-link" href="/logout">Log out</a></li>
				</ul>
			</div>

		</div>


		<div class="row">

			<div class="col"></div>

		</div>

		<div class="row">

			<div class="col"></div>

			<div class="col-6">

				<ul class="nav nav-tabs" id="myTab" role="tablist">
					<li class="nav-item"><a class="nav-link active"
						data-toggle="tab" href="#login" role="tab">Note 1</a></li>
					<li class="nav-item"><a class="nav-link" data-toggle="tab"
						href="#signup" role="tab">Note 2</a></li>
				</ul>

				<div class="tab-content">
					<div id="login" class="tab-pane fade show active">

						<p>Note 1</p>

					</div>

					<div id="signup" class="tab-pane fade">

						<p>Note 2</p>

					</div>
				</div>
			</div>

			<div class="col"></div>

		</div>

	</div>

</div>

{{>footer.hbs}}
